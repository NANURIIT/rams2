                , LAST_RDMP_TMRD                          /* 최종상환회차 */
                , DEAL_ISTM_BLCE                          /* 할부잔액 */
                , DEAL_EQL_RDMP_AMT                       /* 균등상환금액 */
                , ISTM_DTM_RDMP_AMT                       /* 할부일시상환금액 */
                , RCVB_INTR_AMT                           /* 미수이자금액 */
                , GRTE_DCD                                /* 보증구분코드 */
                , PYMT_GRTE_RFR_NO                        /* 지급보증참조번호 */
                , GRTE_ISTT_CD                            /* 보증기관코드 */
                , GRTE_ISTT_NM                            /* 보증기관명 */
                , BUY_SHQT                                /*  매수좌수 */
                , SLL_SHQT                                /*  매도좌수 */
                , AVR_UNPR                                /*  평균단가 */
